[Transfer]
Features=dev
ProtectVersion=%A

[Source]
Type=url-file
Path=https://extensions.snowlinux.org/extensions/
MatchPattern=sysext-dev_@v_%a.raw

[Target]
InstancesMax=2
Type=regular-file
Path=/var/lib/extensions.d/
MatchPattern=sysext-dev_@v_%a.raw
CurrentSymlink=/var/lib/extensions/sysext-dev.raw
